                android:id="@+id/lblFilaVisualizarME"
                android:layout_width="wrap_content"
                android:layout_marginLeft="12dp"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="asdf"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:textStyle="bold"
                android:text="@string/HoraSalida"/>

            <TextView
                android:id="@+id/lblFilaVisualizarHS"
                android:layout_width="wrap_content"
                android:layout_marginLeft="12dp"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="asdf"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_marginLeft="12dp"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text=":"/>

            <TextView
                android:id="@+id/lblFilaVisualizarMS"
                android:layout_width="wrap_content"
                android:layout_marginLeft="12dp"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="asdf"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:textStyle="bold"
                android:text="@string/tlf1"/>

            <TextView
                android:id="@+id/lblFilaVisualizarTlf1"
                android:layout_width="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="s"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/tlf2"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/lblFilaVisualizarTlf2"
                android:layout_width="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="s"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:textStyle="bold"
                android:text="@string/Tlf3"/>

            <TextView
                android:id="@+id/lblFilaVisualizarTlf3"
                android:layout_width="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="s"/>

        </LinearLayout>


    </LinearLayout>

</LinearLayout>