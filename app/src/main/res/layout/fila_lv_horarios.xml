                android:layout_marginLeft="12dp"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:text="asdf"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:textStyle="bold"
                android:text="@string/HoraSalida"/>

            <TextView
                android:id="@+id/lblFilaHSalida"
